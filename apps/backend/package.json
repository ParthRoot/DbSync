{
    "name": "@klarluft/backend",
    "version": "1.0.0",
    "description": "Backend of AMBG",
    "author": {
        "name": "scaletech team",
        "email": "info@scaletech.xyz"
    },
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "node dist/main",
        "start:debug": "nest start --watch --debug 0.0.0.0:9228",
        "start:prod": "node dist/main",
        "eslint:check": "eslint \"./src/**/*.ts\"",
        "eslint:fix": "npm run eslint:check -- --fix",
        "lint": "npm run eslint:check && npm run prettier:write",
        "precommit": "pnpm run lint",
        "prettier:check": "prettier --check ./src/**/*.{ts,js,json,*rc}",
        "prettier:write": "prettier --write ./src/**/*.ts",
        "test": "jest --config ./test/jest.config.ts",
        "test:ci": "jest --config ./test/jest.config.ts --ci --coverage --reporters=default --reporters=jest-junit",
        "test:watch": "jest --config ./test/jest.config.ts --watch",
        "test:cov": "jest --config ./test/jest.config.ts --coverage",
        "test:debug": "node --config ./test/jest.config.ts --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:create": "npm run typeorm migration:create src/migrations/$FOR",
        "migration:generate": "pnpm run typeorm migration:generate ./src/migrations/$FOR -d ./src/utils/database/connection/index.ts",
        "migration:up": "NODE_OPTIONS=--max-old-space-size=1024 ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/utils/database/connection/index.ts",
        "migration:up-fake": "ts-node ./node_modules/typeorm/cli.js -d ./src/utils/database/connection/index.ts migration:run --fake",
        "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/utils/database/connection/index.ts",
        "seed": "ts-node ./src/seeds/seed.run.ts"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/schedule": "^4.0.1",
        "@nestjs/typeorm": "^9.0.1",
        "aws-sdk": "^2.1272.0",
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "compression": "^1.7.4",
        "csvtojson": "^2.0.10",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "express-correlation-id": "^2.0.1",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "luxon": "^3.4.4",
        "moment": "^2.29.4",
        "mssql": "^10.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "typeorm": "^0.3.10"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/better-sqlite3": "^7.6.3",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/jest": "29.4.0",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/mocha": "^10.0.0",
        "@types/mssql": "^9.1.5",
        "@types/node": "^18.11.9",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "better-sqlite3": "^8.0.1",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "29.3.1",
        "jest-junit": "^15.0.0",
        "node-mocks-http": "^1.12.1",
        "pg-mem": "^2.6.4",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "29.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.1.1",
        "typescript": "^4.7.4"
    }
}
